<% layout('/layouts/boilerplate') -%>

        <div class="row">
            <% for (let listing of dataList) { %>
                <div class="card homePage">
                    <a href="/listing/<%=listing._id%>">
                    <img src="<%= listing.image.url %>" class="card-img-top" alt="listing_img">
                    <div class="card-img-overlay"></div>
                    <div class="card-body">
                        <h5 class="card-title">
                            <%= listing.title %>
                        </h5>
                        <p class="card-description">
                            <%=listing.desc%>
                        </p>
                        <p class="card-price"> <b>&#8377;<%=listing.price.toLocaleString('en-IN')%> </b>/day</p>
                    </div>
                    </a>
                </div>
                <% } %>
        </div>